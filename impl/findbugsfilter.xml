<FindBugsFilter>
    <Match>
        <Class name="~.*_\$logger$" />
        <Bug pattern="NM_CLASS_NAMING_CONVENTION" />
    </Match>
    <Match>
        <Or>
            <Bug pattern="SIC_INNER_SHOULD_BE_STATIC_ANON" />
            <Bug pattern="RI_REDUNDANT_INTERFACES" />
        </Or>
    </Match>
</FindBugsFilter>
